class Manusia(object):
    nama = None

    def __init__(self, nama):
        self.nama = nama

    def makan(self):
        print('{} makan nasi'.format(self.nama))


class ManusiaMilienial(Manusia):
    nama = None
    email = None

    def __init__(self, nama):
        self.nama = nama

    def makan(self):
        print('{} makan nasi'.format(self.nama))

    def set_email(self, email):
        self.email = email


programmer = ManusiaMilienial('Eka')
programmer.set_email('eka@test.com')
programmer.makan()

petani = ManusiaMilienial('Putra')
petani.set_email('putrs@test.com')
petani.makan()
